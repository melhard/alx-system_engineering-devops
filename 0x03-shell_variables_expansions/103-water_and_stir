#!/bin/bash
awk 'BEGIN{w=ARGV[1];s=ARGV[2];split("ewatr",a,"");split("tiqrfs",b,"");split("bestchol",c,"");for(i in a)A[a[i]]=i-1;for(i in b)B[b[i]]=i-1;x=y=0;for(i=1;i<=length(w);i++)x=5*x+A[substr(w,i,1)];for(i=1;i<=length(s);i++)y=6*y+B[substr(s,i,1)];z=x+y;r="";while(z){r=c[(z%8)+1]r;z=int(z/8)}print r}' "$WATER" "$STIR"
